VIEW "INC_M_SESSIONUSERDT"
AS SELECT EMPID,EMPNM,EMAIL,MGRID,MGRNM,USRID,POSIT,DEPTC,DEPNM,PHONE,'UPI-'||EMPID as EMUPI,VCPNM,EUNIT,ECITY
            FROM "INC_M_EMPDT" 
           -- WHERE LOWER(USRID) = LOWER(SESSION_USER) AND ISDEL='0';
            WHERE LOWER(EMPID) = LOWER('97203409') AND ISDEL='0';