VIEW INC_OHS_OfficialTravelMission AS
SELECT 
    OTR.OFTID,
    OTR.INCID,
    OTR.RFTVL,
    MST.TXVAL RFTVL_TXT,
    OTR.INCLC,
    OTR.LCDES,
    OTR.LOCIN,
    OTR.ACTTI,
    OTR.WIRMT
FROM 
    "INC_T_OTRMS" AS OTR 
LEFT JOIN "INC_M_MSTDT" MST ON MST.UNQID = OTR.RFTVL
WHERE 
    OTR.ISDEL = '0';