VIEW INC_OHS_REPORTSIGNOFF AS
SELECT 
    SIG.SIGID, 
    SIG.INCID, 
    SIG.INVID, 
    SIG.RSIGN, 
    SIG.SFONT, 
    SIG.SOLAG, 
    SIG.NOTES, 
    SIG.EMPID,
    EMP.EMPNM AS SIGNM
FROM 
    "INC_T_SIGOF" SIG
LEFT JOIN "INC_M_EMPDT" EMP ON EMP.EMPID=SIG.EMPID
WHERE SIG.ISDEL='0';