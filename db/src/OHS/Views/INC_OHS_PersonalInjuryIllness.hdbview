VIEW INC_OHS_PersonalInjuryIllness AS
SELECT 
    PIJ.PRIID,
    PIJ.INCID,
    PIJ.INJID,
    PIJ.LTRTQ,
    MST.TXVAL LTRTQ_TXT,
    PIJ.WTIMF,
    PIJ.WBBFE,
    PIJ.WBSPT,
    PIJ.WHDTE,
    PIJ.TOBBF,
    MST1.TXVAL TOBBF_TXT,
    PIJ.OTHER
FROM 
    "INC_T_PIJIL" PIJ 
LEFT JOIN "INC_M_MSTDT" MST  ON MST.UNQID = PIJ.LTRTQ
LEFT JOIN "INC_M_MSTDT" MST1 ON MST1.UNQID = PIJ.TOBBF
WHERE 
    PIJ.ISDEL = '0';