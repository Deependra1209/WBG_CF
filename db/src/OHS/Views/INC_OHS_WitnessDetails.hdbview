VIEW INC_OHS_WitnessDetails AS
SELECT 
    WIT.WITID,
    WIT.INCID,
    WIT.WITTP,
    MST.TXVAL ,
    WIT.WITNM,
    WIT.WITPN,
    WIT.WITEM,
    MST.TXVAL AS WITTP_TXT,
    WIT.NRMID
FROM 
    "INC_T_WITDT" WIT 
LEFT JOIN "INC_M_MSTDT" MST ON MST.UNQID = WIT.WITTP
WHERE 
    WIT.ISDEL = '0';