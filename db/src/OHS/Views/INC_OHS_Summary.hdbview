VIEW INC_OHS_Summary AS
SELECT EVN.EVNID,EVN.INCID,EVN.SECTN,EVN.CRTDT as UDATE,EVN.CRTTM as UTIME,EMP.EMPNM as UPDBY,EVN.DETLS, 
             EVN.EVDES as NOTES,EVN.PRFLW 
             FROM "INC_T_EVNSM" EVN
             INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID=EVN.INCID AND INC.ISDEL='0'
             --LEFT JOIN "INC_M_EMPDT" AS EMP ON LOWER(EMP.USRID)=LOWER(EVN.CRTBY) --AND EMP.ISDEL='0'
            LEFT JOIN "INC_M_EMPDT" AS EMP ON LOWER(EMP.USRID)=LOWER('SLS1209') AND EMP.ISDEL='0'--EVN.CRTBY
             WHERE EVN.ISDEL='0'; 