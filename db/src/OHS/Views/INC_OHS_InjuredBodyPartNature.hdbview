VIEW "INC_OHS_INJUREDBODYPARTNATURE" AS
SELECT 
    IBP.IBDID,
    IBP.INCID,
    IBP.PRIID,
    IBP.BODPT,
    MST.TXVAL AS BODPT_TXT,
    IBP.NOIIL,
    MST1.TXVAL AS NOIIL_TXT,
    IBP.SIDEY,
    MST2.TXVAL AS SIDEY_TXT
FROM 
    "INC_T_IBPNI" AS IBP 
LEFT JOIN "INC_M_MSTDT" AS MST  ON MST.UNQID = IBP.BODPT
LEFT JOIN "INC_M_MSTDT" AS MST1 ON MST1.UNQID = IBP.NOIIL
LEFT JOIN "INC_M_MSTDT" AS MST2 ON MST2.UNQID = IBP.SIDEY
WHERE 
    IBP.ISDEL = '0';