VIEW INC_OHS_PREINVESTIGATION AS
SELECT 
PRI.PRIID,
PRI.INCID,
PRI.ISMLC,
PRI.INLOC,
PRI.INJCD,
PRI.IJDEQ,
PRI.KNOHZ,
PRI.HZDES,
PRI.WECON,
PRI.ILEVL,
MST1.TXVAL AS ILEVLTXT,
PRI.STAFF,
PRI.WITSS,
PRI.SKTCH,
PRI.PNOTE,
PRI.ISSVD,
PRI.PICOM

FROM  "INC_T_PRINV" PRI 

LEFT JOIN "INC_M_MSTDT" MST1 ON MST1.UNQID=PRI.ILEVL AND MST1.ISDEL = '0'
WHERE 
   PRI.ISDEL ='0';