VIEW INC_OHS_Transportation AS
SELECT 
    TRN.TRSID,
    TRN.INCID,
    TRN.TYPTR,
    MST.TXVAL TYPTR_TXT,
    TRN.LOCXX,
    TRN.SPEXX,
    TRN.POLIV,
    TRN.ACTTI,
    TRN.OPASS,
    TRN.INJUR,
    TRN.OVCXX,
    TRN.VEIID,
    TRN.MDLYR,
    TRN.VECDS,
    TRN.PCVXX
FROM 
    "INC_T_TRNPT" TRN 
LEFT JOIN "INC_M_MSTDT" MST ON MST.UNQID = TRN.TYPTR
WHERE 
    TRN.ISDEL = '0';