COLUMN TABLE "INC_M_SCATA" (
    "SCTID" INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START WITH 1 INCREMENT BY 1) COMMENT 'Primary Key Of The Table', 
    "UNQID" INTEGER COMMENT 'Unique Id', 
    "TYVAL" NVARCHAR(50) COMMENT 'Type Value', 
    "TXVAL" NVARCHAR(100) COMMENT 'Text Value', 
    "ISDEL" NVARCHAR(1),
    "CRTBY" NVARCHAR(100),
    "CHNBY" NVARCHAR(100), 
    "PRNID" INTEGER  COMMENT 'Parent Id', 
    "SEQID" SMALLINT  COMMENT 'Sequence Id for Sorting'
);