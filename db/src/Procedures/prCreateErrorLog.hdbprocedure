PROCEDURE "prCreateErrorLog" (
    IN APPNM NVARCHAR(50),
    IN SRVNM NVARCHAR(50),
    IN PLOAD NVARCHAR(5000),
    IN ERTXT NVARCHAR(5000),
    IN MDLNM NVARCHAR(50),
    IN USRID NVARCHAR(100)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 
        INSERT INTO INC_T_ERLOG (APPNM,SRVNM,PLOAD,ERTXT,MDLNM,USRID, ISDEL, CRTBY, CRTDT, CRTTM) 
        VALUES (:APPNM, :SRVNM, :PLOAD, :ERTXT, :MDLNM, :USRID, '0', SESSION_USER, CURRENT_DATE, CURRENT_TIME);
END;