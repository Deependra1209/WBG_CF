PROCEDURE "prUpdateCloseCA" (
    IN CRAID INTEGER,
    IN INCID INTEGER,
    IN CASTS INTEGER,
    IN CNOTE NVARCHAR(200),
    IN COMDT DATE	
    
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
        UPDATE INC_T_CRACT
        SET CASTS = :CASTS,CNOTE = :CNOTE, COMDT = :COMDT
        WHERE CRAID = :CRAID AND INCID = :INCID AND ISDEL = '0';
END;