PROCEDURE "prUpdateIncidentNumber" (
    IN INCID INTEGER,         -- Unique id
    IN INCNM NVARCHAR(20)     -- Incident id in inc-year-001 format
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 

    UPDATE "INC_T_INCDT" 
    SET  INCNM = :INCNM, CHNBY = SESSION_USER, CHNDT = CURRENT_DATE, CHNTM = CURRENT_TIME
    WHERE INCID = :INCID AND ISDEL = '0';

END;
