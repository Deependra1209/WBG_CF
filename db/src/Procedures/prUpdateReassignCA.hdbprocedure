PROCEDURE "prUpdateReassignCA" (
    IN CRAID INTEGER,
    IN INCID INTEGER,
    IN ASSID NVARCHAR(30),
    IN RASNT NVARCHAR(1000)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
        UPDATE INC_T_CRACT
        SET ASSID = :ASSID, RASNT = :RASNT
        WHERE CRAID = :CRAID AND INCID = :INCID AND ISDEL = '0';
END;