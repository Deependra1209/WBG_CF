PROCEDURE "prCreateEventSummary" (
    IN INCID INTEGER,
    IN PRFLW INTEGER,
    IN ISRED INTEGER,
    IN SECTN NVARCHAR(50),
    IN DETLS NVARCHAR(100),
    IN EVDES NVARCHAR(5000)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 
        INSERT INTO INC_T_EVNSM (INCID, PRFLW, ISRED, SECTN, DETLS, EVDES, ISDEL, CRTBY, CRTDT, CRTTM) 
        VALUES (:INCID, :PRFLW, :ISRED, :SECTN, :DETLS, :EVDES, '0', SESSION_USER, CURRENT_DATE, CURRENT_TIME);
END;
