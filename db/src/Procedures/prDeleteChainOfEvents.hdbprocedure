PROCEDURE "prDeleteChainOfEvents" (
    IN EIJID INTEGER,        -- Unique id 
    IN INCID INTEGER         -- Incident Id
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 

    UPDATE "INC_T_EVINJ" 
    SET  ISDEL = '1', CHNBY = SESSION_USER, CHNDT = CURRENT_DATE, CHNTM = CURRENT_TIME
    WHERE INCID = :INCID AND EIJID = :EIJID;

END;