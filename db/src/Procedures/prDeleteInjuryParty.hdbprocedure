PROCEDURE "prDeleteInjuryParty" (
    IN INJID INTEGER,         -- Unique id
    IN INCID INTEGER
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 

    UPDATE "INC_T_INJDT" 
    SET  ISDEL = '1', CHNBY = SESSION_USER, CHNDT = CURRENT_DATE, CHNTM = CURRENT_TIME
    WHERE INCID = :INCID AND INJID = :INJID;

END;
