PROCEDURE "prCreateAuditLog" (
    IN INCID INTEGER,
    IN REFID INTEGER,
    IN RTYPE NVARCHAR(100),
    IN SRVNM NVARCHAR(200),
    IN PLOAD NVARCHAR(5000),
    IN VIENM NVARCHAR(50)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 
AS 
BEGIN 
        INSERT INTO INC_T_AULOG (INCID, REFID, RTYPE, SRVNM, PLOAD, VIENM, ISDEL, CRTBY, CRTDT, CRTTM) 
        VALUES (:INCID, :REFID, :RTYPE, :SRVNM, :PLOAD, :VIENM,  '0', SESSION_USER, CURRENT_DATE, CURRENT_TIME);
END;
